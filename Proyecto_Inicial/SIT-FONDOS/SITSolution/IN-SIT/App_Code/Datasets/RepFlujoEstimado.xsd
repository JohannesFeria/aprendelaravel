<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="RepFlujoEstimado" targetNamespace="http://tempuri.org/RepFlujoEstimado.xsd"
	elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/RepFlujoEstimado.xsd"
	xmlns:mstns="http://tempuri.org/RepFlujoEstimado.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="RepFlujoEstimado" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FlujoEstimado">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CodigoPortafolio" type="xs:string" minOccurs="0" />
							<xs:element name="NumeroSecuencial" type="xs:string" minOccurs="0" />
							<xs:element name="SaldoAnterior" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoPosterior" type="xs:decimal" minOccurs="0" />
							<xs:element name="Egreso" type="xs:string" minOccurs="0" />
							<xs:element name="EgresoValor" type="xs:decimal" minOccurs="0" />
							<xs:element name="IngresoValor" type="xs:decimal" minOccurs="0" />
							<xs:element name="ValorOperacion" type="xs:decimal" minOccurs="0" />
							<xs:element name="TipoMovimiento" type="xs:string" minOccurs="0" />
							<xs:element name="DescripcionPortafolio" type="xs:string" minOccurs="0" />
							<xs:element name="FechaVencimiento" type="xs:string" minOccurs="0" />
							<xs:element name="CodigoTipoOperacion" type="xs:string" minOccurs="0" />
							<xs:element name="DescripcionOperacion" type="xs:string" minOccurs="0" />
							<xs:element name="NumeroOperacion" type="xs:string" minOccurs="0" />
							<xs:element name="CodigoMoneda" type="xs:string" minOccurs="0" />
							<xs:element name="FechaOperacion" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>