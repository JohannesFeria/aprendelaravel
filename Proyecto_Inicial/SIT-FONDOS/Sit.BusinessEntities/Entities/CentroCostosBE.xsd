<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="CentroCostosBE" targetNamespace="http://tempuri.org/CentroCostosBE.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/CentroCostosBE.xsd" xmlns:mstns="http://tempuri.org/CentroCostosBE.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata"
	xmlns:msprop="urn:schemas-microsoft-com:xml-msprop">
	<xs:element name="CentroCostosBE" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="CentroCostos">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CodigoCentroCosto" type="xs:string" minOccurs="0" />
							<xs:element name="NombreCentroCosto" type="xs:string" minOccurs="0" />
							<xs:element name="Situacion" type="xs:string" minOccurs="0" />
							<xs:element name="Host" type="xs:string" minOccurs="0" />
							<xs:element name="UsuarioCreacion" type="xs:string" minOccurs="0" />
							<xs:element name="FechaCreacion" type="xs:decimal" minOccurs="0" />
							<xs:element name="HoraCreacion" type="xs:string" minOccurs="0" />
							<xs:element name="UsuarioModificacion" type="xs:string" minOccurs="0" />
							<xs:element name="FechaModificacion" type="xs:decimal" minOccurs="0" />
							<xs:element name="HoraModificacion" type="xs:string" minOccurs="0" />
							<xs:element name="UsuarioEliminacion" type="xs:string" minOccurs="0" />
							<xs:element name="FechaEliminacion" type="xs:decimal" minOccurs="0" />
							<xs:element name="HoraEliminacion" type="xs:string" minOccurs="0" />
							<xs:element name="LimaProvincia" type="xs:decimal" minOccurs="0" msprop:nullValue="0" />
							<xs:element name="TipoGasto" type="xs:decimal" minOccurs="0" msprop:nullValue="0" />
							<xs:element name="Estado" type="xs:decimal" minOccurs="0" msprop:nullValue="0" />
							<xs:element name="Activo" type="xs:string" minOccurs="0" />
							<xs:element name="NumeroTrabajadores" type="xs:decimal" minOccurs="0" msprop:nullValue="0" />
							<xs:element name="Direccion" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>